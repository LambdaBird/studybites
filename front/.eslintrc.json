{
  "extends": ["react-app", "airbnb","plugin:jsx-a11y/recommended","prettier"],
  "plugins": ["react","jsx-a11y"],
  "rules": {
    "import/prefer-default-export": "off",
    "react/jsx-props-no-spreading": "off"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx"]
      },
      "alias": {
        "map": [["@sb-ui", "./src"]]
      }
    }
  }
}
